<main>
  <h1>Browser</h1>
  <form action="" method="">
    <input value="{{search}}" type="text" class="search" name="search" placeholder="Search...">
    <input value="{{from}}" type="number" name="from" class="difficulty" placeholder="Difficulty from...">
    <span>-</span>
    <input value="{{to}}" type="number" name="to" class="difficulty" placeholder="Difficulty to...">
    <input type="submit" value="search">
  </form>
  {{#each cubes}}
    {{>cube}}
  {{/each}}
</main>